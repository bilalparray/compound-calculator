{
  "expo": {
    "name": "Compounding Calculator",
    "slug": "compound-calculator",
    "version": "0.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "compoundingcalculator",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bilalparray07.compoundingcalculator",
      "versionCode": 4
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3821692834936093~7004860921",
          "iosAppId": "ca-app-pub-3821692834936093~7004860921"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8c3b6cb1-d61a-433d-b62b-2bac2b77ecde"
      }
    },
    "react-native-google-mobile-ads": {
      "androidAppId": "ca-app-pub-3821692834936093~7004860921"
    }
  }
}
